<div class="row">
  <div class="well col-md-8 col-md-offset-2">
    <%= form_for @article do |f| %>
    
      <%= f.label :title %>
      <%= f.text_field :title %>
      
      <%= f.label :body %>
      <%= f.text_field :body %>
      
      <%= f.submit @article 'Create Article' "btn btn-success") %>
      
    <% end %>
  </div>
</div>
